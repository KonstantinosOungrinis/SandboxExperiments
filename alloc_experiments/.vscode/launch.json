    "version": "0.2.0",
    "configurations": [
        {
            "name": "Plot",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/Plots",
            "program": "Plot.py",
            "console": "integratedTerminal"
        },
        {
            "name": "RunAll",
            "type": "debugpy",
            "request": "launch",
            "program": "RunAll.py",
            "console": "integratedTerminal"
        },
        {
            "name": "default",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bench_default",
            "args": [
                "1",
                "7",
                "Alloc"
            ],
            "cwd": "${fileDirname}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                }
            ],
            "preLaunchTask": "make"
        },
        {
            "name": "malloc",
